<div role="form">
  <%= form_for @domain do |d| %>
      <div class="form-group">
        <%= d.label :DomainName %><br>
        <%= d.text_field :name , class:'form-control' %>
      </div>
      <div class="form-group">
        <%= d.submit :Save , class:'btn btn-primary'%>
      </div>
  <% end %>
</div>
